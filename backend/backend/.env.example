# Excel Download Path
DOWNLOAD_PATH=/app/Data/ExcelFiles

# Screenshot Path (optional - defaults to DOWNLOAD_PATH/screenshots)
SCREENSHOT_PATH=/app/Data/Screenshots

# Database Connection
DB_HOST=localhost
DB_PORT=5432
DB_NAME=wiser
DB_USER=postgres
DB_PASSWORD=your_password_here

# JWT Configuration
JWT__SECRET=your_jwt_secret_minimum_32_characters_long
JWT__ISSUER=wiser-api
JWT__AUDIENCE=wiser-app

# Google OAuth Configuration
GOOGLE__CLIENT_ID=your_google_client_id_from_console
GOOGLE__CLIENT_SECRET=your_google_client_secret_from_console

# Frontend URL
FRONTEND__URL=http://localhost:5173
